<h1>Listing routes</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Url</th>
    <th>Rid</th>
    <th>Grade</th>
    <th>Set by</th>
    <th>Gym</th>
    <th>Location</th>
    <th>Created Date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @routes.each do |route| %>
  <tr>
    <td><%= route.name %></td>
    <td><%= route.url %></td>
    <td><%= route.rid %></td>
    <td><%= route.grade %></td>
    <td><%= route.set_by %></td>
    <td><%= route.gym %></td>
    <td><%= route.location %></td>
    <td><%= route.created_at %></td>
    <td><%= link_to 'Show', route %></td>
    <td><%= link_to 'Edit', edit_route_path(route) %></td>
    <td><%= link_to 'Destroy', route, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Route', new_route_path %>
